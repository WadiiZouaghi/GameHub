{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "vercel-php@0.6.0",
      "config": {
        "document_root": "public",
        "php_version": "8.3"
      }
    }
  ],
  "routes": [
    {
      "src": "^/index\\.php(/|$)",
      "use": "vercel-php@0.6.0"
    },
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ]
}
