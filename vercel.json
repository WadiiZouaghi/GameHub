{
  "version": 2,
  "buildCommand": "composer install --no-dev && php bin/console cache:clear",
  "builds": [
    {
      "src": "public/index.php",
      "use": "vercel-php@0.7.2",
      "config": {
        "document_root": "public",
        "php_version": "8.3"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/index.php"
    }
  ],
  "env": {
    "APP_ENV": "prod"
  }
}
